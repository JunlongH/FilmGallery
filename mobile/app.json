{
  "expo": {
    "name": "FilmGallery",
    "slug": "film-gallery-mobile",
    "version": "1.9.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.filmgallery.app"
    },
    "android": {
      "package": "com.filmgallery.app",
      "versionCode": 7,
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./plugins/withNetworkSecurityConfig",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "minSdkVersion": 26
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow FilmGallery to access your photos.",
          "saveToPhotosPermission": "Allow FilmGallery to save photos to your gallery.",
          "isAccessMediaLocationEnabled": false
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Allow FilmGallery to access your camera for professional light metering.",
          "enableMicrophonePermission": false,
          "enableFrameProcessors": true
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow FilmGallery to use your location for tagging shots."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f43f4bde-60d1-41af-acdc-6be0a3fb94f0"
      }
    }
  }
}
